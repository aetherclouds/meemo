{
  "name": "meemo",
  "description": "A couple versatile tools aimed at language learners!",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "assets/icons/ext-icon48.png",
    "128": "assets/icons/ext-icon128.png"
  },
  "browser_action": {},
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "src/html/options.html",
    "open_in_tab": false
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "src/js/contentScript/content.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "assets/**/*.js",
    "assets/*.css",
    "assets/*.js",
    "assets/Content.css",
    "assets/Content.js",
    "assets/ankiConnectUtil.js",
    "assets/const.js",
    "assets/data/dicts/*.csv",
    "assets/data/flags/*.svg",
    "assets/icons/*",
    "assets/src/js/contentScript/content.js",
    "assets/svelteTransition.js"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "azethercloudschrm@gmail.com"
    }
  }
}